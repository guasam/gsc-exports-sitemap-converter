{
  "name": "ghpage-react-env",
  "version": "0.1.0",
  "description": "Minimal boilerplate for writing Web Applications using React, Webpack & TypeScript in 2021",
  "license": "MIT",
  "homepage": "http://guasam.github.io/myapp",
  "author": {
    "name": "Guasam",
    "url": "https://github.com/guasam"
  },
  "scripts": {
    "wt": "wtlauncher.bat",
    "start": "cross-env NODE_ENV=development webpack serve --config tools/webpack/webpack.dev.js",
    "build": "cross-env NODE_ENV=production webpack --config tools/webpack/webpack.prod.js",
    "lint": "eslint src/ --ext .ts,.js,.tsx,.jsx",
    "predeploy": "npm run build",
    "deploy": "gh-pages -d build"
  },
  "devDependencies": {
    "@marshallofsound/webpack-asset-relocator-loader": "^0.5.0",
